{
  "name": "@tecsinapse/chat-component",
  "version": "2.1.3",
  "license": "MIT",
  "main": "build/index.js",
  "module": "build/index.js",
  "files": [
    "build/"
  ],
  "scripts": {
    "prestart": "yarn install",
    "start": "react-scripts start",
    "build:dev": "rimraf -rf build/ && REACT_APP_ENV=development react-scripts build",
    "build": "rimraf -rf build/ && REACT_APP_ENV=production react-scripts build",
    "build:nonsplit": "rimraf -rf build/ && REACT_APP_ENV=production node ./scripts/buildNonSplit.js",
    "build:win": "rimraf -rf build/ && SET \"REACT_APP_ENV=production\" && node ./scripts/buildNonSplit.js",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "build:lib": "rimraf -rf build/ && babel src/ -d build/",
    "eslint:fix": "eslint ./src --ext .js --fix",
    "prettify": "prettier --write src/**/*.{js,json} && prettier --write *.{js,json}"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "post-commit": "git update-index --again"
    }
  },
  "lint-staged": {
    "*.js": [
      "yarn eslint:fix",
      "yarn prettify",
      "git add ."
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version",
      "ie 11"
    ]
  },
  "dependencies": {
    "@material-ui/core": "^4.11.1",
    "@material-ui/styles": "^4.11.1",
    "@mdi/js": "^5.9.55",
    "@mdi/react": "^1.4.0",
    "@tecsinapse/chat": "^3.2.7",
    "@tecsinapse/table": "^4.0.2",
    "@tecsinapse/ui-kit": "5.6.7",
    "enumify": "^1.0.4",
    "jwt-simple": "^0.5.6",
    "moment": "^2.27.0",
    "moment-timezone": "^0.5.33",
    "react-app-polyfill": "^1.0.6",
    "react-ga4": "^1.4.0",
    "react-stomp": "^5.0.0",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.1",
    "@babel/core": "^7.12.3",
    "@babel/plugin-syntax-class-properties": "^7.12.13",
    "@babel/plugin-transform-modules-commonjs": "^7.12.1",
    "@babel/plugin-transform-react-jsx": "^7.12.1",
    "@babel/plugin-transform-runtime": "^7.12.1",
    "@babel/preset-env": "^7.12.1",
    "@babel/preset-react": "^7.12.1",
    "@tecsinapse/eslint-config-tecsinapse-js": "^3.0.3",
    "babel-plugin-inline-react-svg": "^2.0.1",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-react-app": "^5.2.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react-hooks": "^4.0.8",
    "husky": "^4.3.0",
    "lint-staged": "^10.4.0",
    "prettier": "^2.1.2",
    "prettier-plugin-packagejson": "^2.2.5",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-scripts": "^3.4.1",
    "rewire": "^5.0.0",
    "rollup-plugin-node-builtins": "^2.1.2"
  },
  "publishConfig": {
    "registry": "https://nexus.portaltecsinapse.com.br/repository/npm-private/"
  }
}
